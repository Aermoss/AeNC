include "ioaenc"

func main() {
    var destroyed = false

    while (not var destroyed) {
        var command = ioaenc.input(">")

        if (var command == "exit") {
            ioaenc.print("Exiting...")
            var destroyed = true
        }

        if (var command != "exit") {
            system(var command)
        }
    }
}
